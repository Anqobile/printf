#include <stdio.h>

/**
  *main - entry point
  *Return: always 0 (sucess)
  */
void printString(const char *str)
{
    const unsigned char *ptr = (const unsigned char *)str;

    while (*ptr != '\0') {
        if (*ptr >= 32 && *ptr < 127)
{
            putchar(*ptr);
        } else
{
            printf("\\x%02X", *ptr);
        }
        ptr++;
    }
}

int main() 
{
    char str[] = "Hello, \nWorld!\t";

    printf("%S\n", str);

    return 0;
}
